msgid "Validation Summary:"
msgstr "Сводка валидации:"

#: src/linter.js:193
msgid "Code"
msgstr "Код"

#: src/linter.js:199
msgid "Message"
msgstr "Сообщение"

#: src/linter.js:205
msgid "Description"
msgstr "Описание"

#: src/linter.js:211
msgid "File"
msgstr "Файл"

#: src/linter.js:217
msgid "Line"
msgstr "Строка"

