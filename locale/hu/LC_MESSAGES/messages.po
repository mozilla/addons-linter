msgstr "Kód"

#: src/linter.js:186
msgid "Message"
msgstr "Üzenet"

#: src/linter.js:192
msgid "Description"
msgstr "Leírás"

#: src/linter.js:198
msgid "File"
msgstr "Fájl"

#: src/linter.js:204
msgid "Line"
msgstr "Sor"

