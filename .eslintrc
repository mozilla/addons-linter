{
  "rules":{
    "no-console": 0
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
  },
  "extends": "amo",
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "node": {
        // This adds ./src for relative imports.
        "moduleDirectory": ["node_modules", "src"]
      }
    }
  }
}
